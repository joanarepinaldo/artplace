{
    "version": 2,
    "builds": [
        {
            "src": "src/server.js",
            "use": "@vercel/node"
        },
        {
          "src": "public/scripts/index.js",
          "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/server.js"
        },
        {
          "src":  "/",
          "dest": "public/scripts/index.js"
        }
    ]
}